<header>
  <h1>Viking Store Analytics Dashboard</h1>
</header>

<main class="container">
  <div class="row">
    <div class="col-sm-6">
      <%= render  partial: 'panel',
                  locals: { panel_data: @overall_platform,
                            title: "1. Overall Platform" } %>
    </div>
    <div class="col-sm-6">
      <%= render  partial: 'panel',
                  locals: { panel_data: @user_demographics,
                            title: "2. User Demographics and Behavior" } %>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-6">
      <%= render  partial: 'panel',
                  locals: { panel_data: @order_statistics,
                            title: "3. Order Statistics" } %>
    </div>
    <div class="col-sm-6">
      <%= render  partial: 'panel',
                  locals: { panel_data: @overall_platform,
                            title: "4. Time Series Data" } %>
    </div>
  </div>
</main>
