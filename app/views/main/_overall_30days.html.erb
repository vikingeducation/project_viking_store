<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <td><strong>Item</strong></td>
      <td><strong>Data</strong></td>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>New Users</td>
      <td><%= @user_created_thirty_days %></td>
    </tr>
    <tr>
      <td>Orders</td>
      <td><%= @order_created_thirty_days %></td>
    </tr>
    <tr>
      <td>New Products</td>
      <td> <%= @product_created_thirty_days %> </td>
    </tr>
    <tr>
      <td>Revenue</td>
      <% @revenue_in_thirty_days.each do |revenue| %>
        <td><%= revenue.sum %></td>
      <% end %>
    </tr>
  </tbody>
</table>