 <h3><%= title %></h3>
<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <tr>
      <th>Item</th>
      <th>User</th>
      <th>Quantity</th>
    </tr>
    <% hash.each_pair do |key, value| %>
      <tr>
        <td> 
        <%= key %> 
        </td>
        <td>
        <%= value["name"] %>
        </td>
        <td>
        <%= value["total"] %>
        </td>
      </tr>
    <% end %>
  </table>
</div>