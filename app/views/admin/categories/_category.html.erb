<%= render :partial => 'shared/table', :locals => {:table => {
  :headers => ['Key', 'Value'],
  :rows => [
    ['ID', category.id],
    ['Name', category.name],
    ['Description', category.description],
    ['Created At', category.created_at.to_formatted_s(:long)],
    ['Updated At', category.updated_at.to_formatted_s(:long)]
  ]
}} %>
