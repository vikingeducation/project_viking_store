<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <% table[:headers].each do |th| %>
        <th>
          <%= th %>
        </th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% table[:rows].each do |row| %>
      <tr>
        <% row.each do |td| %>
          <td>
            <%= td %>
          </td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>