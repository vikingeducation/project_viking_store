<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Logistical Information</h3>
  </div>
  <div class="panel-body">
    <%= render 'order_info' %>
  </div>
</div>
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Order Contents</h3>
  </div>
  <div class="panel-body">
    <%= render partial: 'admin/order_contents/order_contents_table', locals: {order_contents: @order_contents} %>
    <p><%= link_to "Edit Order", edit_admin_order_path(@order) %></p>
    <p><%= link_to "Delete Order", admin_order_path(@order), method: :delete, data: {confirm: 'Are you sure you want to delete this order?'} %></p>
  </div>
</div>